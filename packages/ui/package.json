{
  "name": "ferns-ui",
  "version": "2.0.0",
  "main": "dist/index.js",
  "license": "Apache-2.0",
  "scripts": {
    "local": "tsc-watch --onSuccess \"cp -a dist/* ../flourish/node_modules/ferns-ui/dist\"",
    "lint": "biome check src",
    "lintfix": "biome check --write src",
    "lintfixunsafe": "biome check --write --unsafe src",
    "build": "tsc",
    "dev": "tsc -w",
    "deps": "node checkDeps.js",
    "test": "TZ=America/New_York jest --watchAll",
    "test:ci": "TZ=America/New_York jest --ci",
    "postinstall": "cd ../.. && patch-package",
    "types": "npx typedoc"
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "prettier": {
    "arrowParens": "always",
    "bracketSpacing": false,
    "printWidth": 100,
    "semi": true,
    "singleQuote": false,
    "trailingComma": "es5",
    "useTabs": false
  },
  "files": [
    "dist/**/*",
    "src/**/*"
  ],
  "devDependencies": {
    "@biomejs/biome": "^2.3.6",
    "@expo/config-plugins": "~54.0.4",
    "@testing-library/react-native": "^13.2.0",
    "@types/jest": "^29.5.14",
    "@types/lodash": "^4.17.16",
    "@types/luxon": "^3.6.2",
    "@types/mdurl": "^2.0.0",
    "@types/minimatch": "^6.0.0",
    "@types/react": "~19.1.10",
    "@types/react-datetime-picker": "^5.0.0",
    "@types/react-signature-canvas": "^1.0.5",
    "@types/react-time-picker": "^6.0.0",
    "babel-jest": "^29.7.0",
    "babel-preset-react-app": "^10.0.1",
    "expo-modules-core": "^3.0.29",
    "jest": "^29.7.0",
    "jest-expo": "~53.0.7",
    "jest-github-actions-reporter": "^1.0.3",
    "mdurl": "^2.0.0",
    "prettier": "^3.3.3",
    "react": "19.1.0",
    "react-keyed-flatten-children": "^5.1.1",
    "react-router": "^6.26.2",
    "react-router-dom": "^6.30.1",
    "react-test-renderer": "19.1.0",
    "tsc-watch": "^7.1.1",
    "typescript": "~5.8.3"
  },
  "peerDependencies": {
    "react": "19.1.0",
    "react-router": "^6.26.2",
    "react-router-dom": "^6.30.1"
  },
  "dependencies": {
    "@expo-google-fonts/nunito": "^0.4.2",
    "@expo-google-fonts/titillium-web": "^0.4.1",
    "@expo/vector-icons": "^15.0.3",
    "@react-native-async-storage/async-storage": "2.2.0",
    "@react-native-community/blur": "^4.4.0",
    "@react-native-community/datetimepicker": "8.6.0",
    "@react-native-community/slider": "^5.0.1",
    "@react-native-picker/picker": "2.11.1",
    "emoji-datasource": "^16.0.0",
    "expo-clipboard": "~8.0.8",
    "expo-font": "~14.0.10",
    "expo-haptics": "~15.0.8",
    "expo-image-manipulator": "~14.0.8",
    "expo-image-picker": "~17.0.9",
    "expo-linear-gradient": "~15.0.8",
    "expo-localization": "~17.0.8",
    "libphonenumber-js": "^1.12.31",
    "lodash": "^4.17.21",
    "luxon": "^3.7.2",
    "patch-package": "^8.0.1",
    "react-app-polyfill": "^3.0.0",
    "react-date-picker": "^12.0.1",
    "react-datetime-picker": "^7.0.1",
    "react-dev-utils": "^12.0.1",
    "react-dom": "19.1.0",
    "react-native": "0.81.5",
    "react-native-actions-sheet": "^0.9.3",
    "react-native-calendars": "^1.1312.1",
    "react-native-drawer-layout": "4.2.0",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-google-places-autocomplete": "^2.6.1",
    "react-native-markdown-display": "^7.0.2",
    "react-native-modalize": "^2.1.1",
    "react-native-permissions": "^5.4.4",
    "react-native-picker-select": "^9.1.3",
    "react-native-portalize": "^1.0.7",
    "react-native-reanimated": "~4.1.1",
    "react-native-signature-canvas": "^4.7.4",
    "react-native-svg": "15.12.1",
    "react-native-swiper-flatlist": "^3.2.5",
    "react-native-toast-notifications": "^3.4.0",
    "react-native-webview": "13.15.0",
    "react-native-worklets": "0.5.1",
    "react-signature-canvas": "^1.1.0-alpha.2",
    "react-time-picker": "^7.0.0"
  }
}
